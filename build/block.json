{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "konomotoblock/konomoto-product-block",
  "version": "0.1.0",
  "title": "Konomoto Product Block",
  "category": "theme",
  "icon": "products",
  "keywords": [
    "custom-post-type"
  ],
  "description": "Custom product post type for Konomoto Wood Design",
  "supports": {
    "html": false
  },
  "attributes": {
    "description": {
      "type": "string",
      "default": ""
    },
    "priceEnabled": {
      "type": "boolean",
      "default": true
    },
    "price": {
      "type": "string",
      "default": ""
    },
    "materialsEnabled": {
      "type": "boolean",
      "default": true
    },
    "materials": {
      "type": "array",
      "default": [
        {
          "piece": "",
          "madeFrom": ""
        }
      ]
    },
    "measurementsEnabled": {
      "type": "boolean",
      "default": true
    },
    "measurements": {
      "type": "array",
      "default": [
        {
          "name": "Length:",
          "value": "",
          "unit": "cm"
        },
        {
          "name": "Width:",
          "value": "",
          "unit": "cm"
        },
        {
          "name": "Height:",
          "value": "",
          "unit": "cm"
        },
        {
          "name": "Depth:",
          "value": "",
          "unit": "cm"
        }
      ]
    }
  },
  "textdomain": "konomoto-product-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}